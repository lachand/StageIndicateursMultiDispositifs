#:kivy 1.0.9

<Critere>:
    Scatter:
        do_rotation: True
        do_scale: True
        do_translation: True

<Animal>:
    Scatter:
        do_rotation: True
        do_scale: True
        do_translation: True

<Table>:
    size: root.size
    pos: root.pos

<ZoneUtilisateur>:
    pos: self.position
    size: self.taille
    canvas:
        Color:
            rgb: self.couleur
        Ellipse:
            pos: self.position
            size: self.taille